{
  "name": "logchimp-cli",
  "description": "CLI Tool for installing & updating LogChimp",
  "version": "0.0.0",
  "author": "Yashu Mittal @mittalyashu",
  "bin": {
    "logchimp": "./bin/run"
	},
  "bugs": "https://github.com/logchimp/logchimp-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.0",
    "decompress": "^4.2.1",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "listr2": "^3.1.1",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/logchimp/logchimp-cli",
  "keywords": [
    "logchimp",
    "logchimp-cli",
    "cli"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "logchimp",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 5
    }
  },
  "repository": "logchimp/logchimp-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
