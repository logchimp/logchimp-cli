{
  "name": "logchimp-cli",
  "description": "CLI Tool for installing & updating LogChimp",
  "version": "0.2.1",
  "author": "Yashu Mittal @mittalyashu",
  "bin": {
    "logchimp": "./bin/run"
  },
  "bugs": "https://github.com/logchimp/logchimp-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.0",
    "decompress": "^4.2.1",
    "execa": "^5.0.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.0.0",
    "listr2": "^3.1.1",
    "lodash": "^4.17.20",
    "omgopass": "^3.2.1",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^7.11",
    "eslint-config-oclif": "^3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.1.3",
    "globby": "^11",
    "jest": "^27.0.4",
    "prettier": "^2.3.2"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/logchimp/logchimp-cli",
  "keywords": [
    "logchimp",
    "logchimp-cli",
    "cli"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "logchimp",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 5
    }
  },
  "repository": "logchimp/logchimp-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "jest ./tests/**/*.spec.js",
    "version": "oclif-dev readme && git add README.md",
    "prettier": "prettier --write \"**/*.{js,md}\""
  }
}
